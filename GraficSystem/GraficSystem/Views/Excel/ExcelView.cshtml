

<div class="row" ng-app="ChartApp">
    <div class="col-lg-12 col-xs-12 col-sm-12">
        <!-- BEGIN PORTLET-->
        <div class="portlet light bordered"id="tableDiv">
            <input id="file" type="file" accept=".xlsx" style="visibility:hidden" />
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-bar-chart font-dark hide"></i>
                    <span class="caption-subject font-dark bold uppercase">Importar Datos de Ecxel</span>
                </div>
                <div class="actions">
                    <div class="btn-group btn-group-devided" data-toggle="buttons">
                        <button class="btn red btn-outline btn-circle btn-sm" id="browse">
                            Buscar
                        </button>
                    </div>
                </div>
            </div>
            <div id="xple">
                
            </div>
        </div>
        <!-- END PORTLET-->
    </div>
    <div class="col-lg-12 col-xs-12 col-sm-12" ng-controller="ChartGenerate">
        <!-- BEGIN PORTLET-->
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-bar-chart font-dark hide"></i>
                    <span class="caption-subject font-dark bold uppercase">Graficas</span>
                </div>
                <div class="actions">
                    <div class="btn-group btn-group-devided" data-toggle="buttons">
                        <button class="btn red btn-outline btn-circle btn-sm" ng-click="generate()">
                            Generar
                        </button>
                    </div>
                </div>
            </div>
          
         
            <div id="chart">

            </div>
        </div>
        <!-- END PORTLET-->
    </div>
</div>
@section Scripts{
    <script src="~/Scripts/xlsx.core.min.js"></script>
    <script src="~/Scripts/handsontable.full.js"></script>
    <script src="~/Scripts/angular-js-xlsx.js"></script>
    <script src="~/Scripts/ExcelCharts.js"></script>
   
     }